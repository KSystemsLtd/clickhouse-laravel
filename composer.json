{
    "name": "ksystems/laravel-clickhouse",
    "description": "A service made to provide, set up and use the ClickHouse in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KSystems",
            "email": "support@ksyste.ms"
        }
    ],
    "autoload": {
        "psr-4": {
            "KSystems\\ClickHouse\\": "src/"
        }
    },
    "require": {
        "smi2/phpclickhouse": "^1.3",
        "illuminate/support": "^5.0|^6.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "KSystems\\ClickHouse\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "ClickHouse": "KSystems\\ClickHouse\\Facades\\ClickHouse"
            }
        }
   }
}
